{
  "extends": "airbnb",
  "env": {
    "mocha": true
  },
  "globals": {
    "expect": true
  },
  "rules": {
    "max-len": [2, 79, {
      "ignorePattern": " // eslint-disable-line "
    }],
    "react/jsx-filename-extension": [2, {
      "extensions": [".js"]
    }],
    "react/no-multi-comp": 0,
    "react/prop-types": 0,
    "import/no-extraneous-dependencies": [2, {
      "devDependencies": true
    }]
  }
}
