  },
  "homepage": "https://github.com/relay-tools/relay-local-schema#readme",
  "peerDependencies": {
    "graphql": ">=0.5.0",
    "react-relay": ">=0.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-relay": "^1.0.1",
    "babel-polyfill": "^6.23.0",
